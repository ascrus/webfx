<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="132.0" prefWidth="363.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="buttonClose" defaultButton="true" disable="true" mnemonicParsing="false" text="Close" />
            <Button fx:id="buttonCancel" cancelButton="true" mnemonicParsing="false" text="Cancel" />
        </buttons>
      </ButtonBar>
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <ProgressBar fx:id="progressBar" prefHeight="24.0" prefWidth="343.0" progress="0.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <VBox prefHeight="22.0" prefWidth="330.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="labelFileName" text="File: " VBox.vgrow="ALWAYS" />
            <Label fx:id="labelFileSize" text="Size: " VBox.vgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </top>
</BorderPane>
